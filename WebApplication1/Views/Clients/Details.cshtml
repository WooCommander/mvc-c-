@model ConsoleAppStart.model.Client

@{
    ViewBag.Title = "Details";
}


<div>
    <h4>Клиент</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           ФИО:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FIO)
        </dd>

        <dt>
          Наименование:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NameCompany)
        </dd>

        <dt>
           Адресс:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Adres)
        </dd>

        <dt>
           Номер телефона:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tel)
        </dd>

        <dt>
           Город:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.City)
        </dd>

        <dt>
          Индекс:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Index)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Назад", "Index")
</p>
